		Property IterIncrement                  String  "$iterator++"
		Property IterIncrementForInit           String  "$IterIncrement"
		Property IterIncrementForCleanUp        String  "$IterIncrement"
		Property IterGetCurrent                 String  "$cname[$iterator]"
		Property IterTest                       String  "($iterator < $multiplicity) && $cname[$iterator]"
		Property IterCreate                     String  "$IterType $iterator = 0;"
		Property IterCreateByValue              String  "$IterCreate"
		Property IterReturnType                 String  "$IterType"
		Property IncludeDirective               Enum    "weak, strong" "weak"
	end 
