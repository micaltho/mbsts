	Metaclass StaticArray
		Property RelationTargetType             String  "$(constant)$target$reference"
		Property CType                          String  "$RelationTargetType $cname[$multiplicity]"
		Property FullTypeDefinition             String  "$CType"
		Property Get                            String  "$cname"
		Property Create                         String  ""
		Property CreateByValue                  String  "$CType"
		Property Loop                           String  "for (uint32_t pos = 0U; pos < $multiplicity; ++pos)"
		Property InitInCtorBody                 MultiLine
"$Loop {
	$cname[pos] = nullptr;
}"
		Property Init String ""
		Property Add MultiLine
"$Loop {
	if (!$cname[pos]) {
		$cname[pos] = $item;
		break;
	}
}"
		Property Remove MultiLine
"$Loop {
	if ($cname[pos] == $item) {
		$cname[pos] = nullptr;
	}
}"
		Property RemoveAll                      String  ""
		Property GetAt                          String  "$cname[$index]"
		Property SetAt                          String  "$cname[$index] = $item"
		Property IterType                       String  "uint32_t"
